[32m+[m[32m<?php[m
[32m+[m[32minclude("../conexion.php");[m
[32m+[m
[32m+[m[32m// Obtener los parámetros de paginación[m
[32m+[m[32m$page = $_GET['page'] ?? 1; // Número de página actual[m
[32m+[m[32m$pageSize = $_GET['pageSize'] ?? 5; // Cantidad de elementos por página[m
[32m+[m
[32m+[m[32m// Calcular el índice inicial y la cantidad de registros a obtener[m
[32m+[m[32m$offset = ($page - 1) * $pageSize;[m
[32m+[m[32m$limit = $pageSize;[m
[32m+[m
[32m+[m[32m// Realizar la consulta a la base de datos utilizando los parámetros de paginación[m
[32m+[m[32m$sql = "SELECT * FROM trabajos LIMIT $offset, $limit";[m
[32m+[m[32m$result = mysqli_query($con, $sql);[m
[32m+[m
[32m+[m[32m// Obtener el total de registros sin paginación[m
[32m+[m[32m$total = mysqli_num_rows(mysqli_query($con, "SELECT * FROM trabajos"));[m
[32m+[m
[32m+[m[32m// Construir el arreglo de resultados[m
[32m+[m[32m$results = [[m
[32m+[m[32m  'data' => [],[m
[32m+[m[32m  'total' => $total[m
[32m+[m[32m];[m
[32m+[m
[32m+[m[32m// Recorrer los resultados y agregarlos al arreglo de datos[m
[32m+[m[32mwhile ($row = mysqli_fetch_assoc($result)) {[m
[32m+[m[32m  $results['data'][] = $row;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Devolver los resultados como respuesta JSON[m
[32m+[m[32mheader('Content-Type: application/json');[m
[32m+[m[32mecho json_encode($results);[m
[32m+[m
[32m+[m[32mmysqli_close($con);[m
[32m+[m[32m?>[m
