[32m+[m
[32m+[m[32m    _proto._getScrollTop = function _getScrollTop() {[m
[32m+[m[32m      return this._scrollElement === window ? this._scrollElement.pageYOffset : this._scrollElement.scrollTop;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    _proto._getScrollHeight = function _getScrollHeight() {[m
[32m+[m[32m      return this._scrollElement.scrollHeight || Math.max(document.body.scrollHeight, document.documentElement.scrollHeight);[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    _proto._getOffsetHeight = function _getOffsetHeight() {[m
[32m+[m[32m      return this._scrollElement === window ? window.innerHeight : this._scrollElement.getBoundingClientRect().height;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    _proto._process = function _process() {[m
[32m+[m[32m      var scrollTop = this._getScrollTop() + this._config.offset;[m
[32m+[m
[32m+[m[32m      var scrollHeight = this._getScrollHeight();[m
[32m+[m
[32m+[m[32m      var maxScroll = this._config.offset + scrollHeight - this._getOffsetHeight();[m
[32m+[m
[32m+[m[32m      if (this._scrollHeight !== scrollHeight) {[m
[32m+[m[32m        this.refresh();[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (scrollTop >= maxScroll) {[m
[32m+[m[32m        var target = this._targets[this._targets.length - 1];[m
[32m+[m
[32m+[m[32m        if (this._activeTarget !== target) {[m
[32m+[m[32m          this._activate(target);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (this._activeTarget && scrollTop < this._offsets[0] && this._offsets[0] > 0) {[m
[32m+[m[32m        this._activeTarget = null;[m
[32m+[m
[32m+[m[32m        this._clear();[m
[32m+[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      for (var i = this._offsets.length; i--;) {[m
[32m+[m[32m        var isActiveTarget = this._activeTarget !== this._targets[i] && scrollTop >= this._offsets[i] && (typeof this._offsets[i + 1] === 'undefined' || scrollTop < this._offsets[i + 1]);[m
[32m+[m
[32m+[m[32m        if (isActiveTarget) {[m
[32m+[m[32m          this._activate(this._targets[i]);[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    _proto._activate = function _activate(target) {[m
[32m+[m[32m      this._activeTarget = target;[m
[32m+[m
[32m+[m[32m      this._clear();[m
[32m+[m
[32m+[m[32m      var queries = this._selector.split(',').map(function (selector) {[m
[32m+[m[32m        return selector + "[data-target=\"" + target + "\"]," + selector + "[href=\"" + target + "\"]";[m
[32m+[m[32m      });[m
[32m+[m
[32m+[m[32m      var $link = $__default['default']([].slice.call(document.querySelectorAll(queries.join(','))));[m
[32m+[m
[32m+[m[32m      if ($link.hasClass(CLASS_NAME_DROPDOWN_ITEM)) {[m
[32m+[m[32m        $link.closest(SELECTOR_DROPDOWN).find(SELECTOR_DROPDOWN_TOGGLE).addClass(CLASS_NAME_ACTIVE$2);[m
[32m+[m[32m        $link.addClass(CLASS_NAME_ACTIVE$2);[m
[32m+[m[32m      } else {[m
[32m+[m[32m        // Set triggered link as active[m
[32m+[m[32m        $link.addClass(CLASS_NAME_ACTIVE$2); // Set triggered links parents as active[m
[32m+[m[32m        // With both <ul> and <nav> markup a parent is the previous sibling of any nav ancestor[m
[32m+[m
[32m+[m[32m        $link.parents(SELECTOR_NAV_LIST_GROUP).prev(SELECTOR_NAV_LINKS + ", " + SELECTOR_LIST_ITEMS).addClass(CLASS_NAME_ACTIVE$2); // Handle special case when .nav-link is inside .nav-item[m
[32m+[m
[32m+[m[32m        $link.parents(SELECTOR_NAV_LIST_GROUP).prev(SELECTOR_NAV_ITEMS).children(SELECTOR_NAV_LINKS).addClass(CLASS_NAME_ACTIVE$2);[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      $__default['default'](this._scrollElement).trigger(EVENT_ACTIVATE, {[m
[32m+[m[32m        relatedTarget: target[m
[32m+[m[32m      });[m
