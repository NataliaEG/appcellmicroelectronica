[32m+[m[32m    var _proto = Carousel.prototype;[m
[32m+[m
[32m+[m[32m    // Public[m
[32m+[m[32m    _proto.next = function next() {[m
[32m+[m[32m      if (!this._isSliding) {[m
[32m+[m[32m        this._slide(DIRECTION_NEXT);[m
[32m+[m[32m      }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    _proto.nextWhenVisible = function nextWhenVisible() {[m
[32m+[m[32m      var $element = $__default['default'](this._element); // Don't call next when the page isn't visible[m
[32m+[m[32m      // or the carousel or its parent isn't visible[m
[32m+[m
[32m+[m[32m      if (!document.hidden && $element.is(':visible') && $element.css('visibility') !== 'hidden') {[m
[32m+[m[32m        this.next();[m
[32m+[m[32m      }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    _proto.prev = function prev() {[m
[32m+[m[32m      if (!this._isSliding) {[m
[32m+[m[32m        this._slide(DIRECTION_PREV);[m
[32m+[m[32m      }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    _proto.pause = function pause(event) {[m
[32m+[m[32m      if (!event) {[m
[32m+[m[32m        this._isPaused = true;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (this._element.querySelector(SELECTOR_NEXT_PREV)) {[m
[32m+[m[32m        Util.triggerTransitionEnd(this._element);[m
[32m+[m[32m        this.cycle(true);[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      clearInterval(this._interval);[m
[32m+[m[32m      this._interval = null;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    _proto.cycle = function cycle(event) {[m
[32m+[m[32m      if (!event) {[m
[32m+[m[32m        this._isPaused = false;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (this._interval) {[m
[32m+[m[32m        clearInterval(this._interval);[m
[32m+[m[32m        this._interval = null;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (this._config.interval && !this._isPaused) {[m
[32m+[m[32m        this._updateInterval();[m
[32m+[m
[32m+[m[32m        this._interval = setInterval((document.visibilityState ? this.nextWhenVisible : this.next).bind(this), this._config.interval);[m
[32m+[m[32m      }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    _proto.to = function to(index) {[m
[32m+[m[32m      var _this = this;[m
[32m+[m
[32m+[m[32m      this._activeElement = this._element.querySelector(SELECTOR_ACTIVE_ITEM);[m
[32m+[m
[32m+[m[32m      var activeIndex = this._getItemIndex(this._activeElement);[m
[32m+[m
[32m+[m[32m      if (index > this._items.length - 1 || index < 0) {[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (this._isSliding) {[m
[32m+[m[32m        $__default['default'](this._element).one(EVENT_SLID, function () {[m
[32m+[m[32m          return _this.to(index);[m
[32m+[m[32m        });[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (activeIndex === index) {[m
[32m+[m[32m        this.pause();[m
[32m+[m[32m        this.cycle();[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      var direction = index > activeIndex ? DIRECTION_NEXT : DIRECTION_PREV;[m
[32m+[m
[32m+[m[32m      this._slide(direction, this._items[index]);[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    _proto.dispose = function dispose() {[m
[32m+[m[32m      $__default['default'](this._element).off(EVENT_KEY$2);[m
[32m+[m[32m      $__default['default'].removeData(this._element, DATA_KEY$2);[m
[32m+[m[32m      this._items = null;[m
[32m+[m[32m      this._config = null;[m
[32m+[m[32m      this._element = null;[m
[32m+[m[32m      this._interval = null;[m
[32m+[m[32m      this._isPaused = null;[m
[32m+[m[32m      this._isSliding = null;[m
[32m+[m[32m      this._activeElement = null;[m
[32m+[m[32m      this._indicatorsElement = null;[m
[32m+[m[32m    } // Private[m
[32m+[m[32m    ;[m
[32m+[m
[32m+[m[32m    _proto._getConfig = function _getConfig(config) {[m
[32m+[m[32m      config = _extends({}, Default, config);[m
[32m+[m[32m      Util.typeCheckConfig(NAME$2, config, DefaultType);[m
[32m+[m[32m      return config;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    _proto._handleSwipe = function _handleSwipe() {[m
[32m+[m[32m      var absDeltax = Math.abs(this.touchDeltaX);[m
[32m+[m
[32m+[m[32m      if (absDeltax <= SWIPE_THRESHOLD) {[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      var direction = absDeltax / this.touchDeltaX;[m
[32m+[m[32m      this.touchDeltaX = 0; // swipe left[m
[32m+[m
[32m+[m[32m      if (direction > 0) {[m
[32m+[m[32m        this.prev();[m
[32m+[m[32m      } // swipe right[m
[32m+[m
[32m+[m
[32m+[m[32m      if (direction < 0) {[m
[32m+[m[32m        this.next();[m
[32m+[m[32m      }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    _proto._addEventListeners = function _addEventListeners() {[m
[32m+[m[32m      var _this2 = this;[m
[32m+[m
[32m+[m[32m      if (this._config.keyboard) {[m
[32m+[m[32m        $__default['default'](this._element).on(EVENT_KEYDOWN, function (event) {[m
[32m+[m[32m          return _this2._keydown(event);[m
[32m+[m[32m        });[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (this._config.pause === 'hover') {[m
[32m+[m[32m        $__default['default'](this._element).on(EVENT_MOUSEENTER, function (event) {[m
[32m+[m[32m          return _this2.pause(event);[m
[32m+[m[32m        }).on(EVENT_MOUSELEAVE, function (event) {[m
[32m+[m[32m          return _this2.cycle(event);[m
[32m+[m[32m        });[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (this._config.touch) {[m
[32m+[m[32m        this._addTouchEventListeners();[m
[32m+[m[32m      }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    _proto._addTouchEventListeners = function _addTouchEventListeners() {[m
[32m+[m[32m      var _this3 = this;[m
[32m+[m
[32m+[m[32m      if (!this._touchSupported) {[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      var start = function start(event) {[m
[32m+[m[32m        if (_this3._pointerEvent && PointerType[event.originalEvent.pointerType.toUpperCase()]) {[m
[32m+[m[32m          _this3.touchStartX = event.originalEvent.clientX;[m
[32m+[m[32m        } else if (!_this3._pointerEvent) {[m
[32m+[m[32m          _this3.touchStartX = event.originalEvent.touches[0].clientX;[m
[32m+[m[32m        }[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      var move = function move(event) {[m
[32m+[m[32m        // ensure swiping with one touch and not pinching[m
[32m+[m[32m        if (event.originalEvent.touches && event.originalEvent.touches.length > 1) {[m
[32m+[m[32m          _this3.touchDeltaX = 0;[m
[32m+[m[32m        } else {[m
[32m+[m[32m          _this3.touchDeltaX = event.originalEvent.touches[0].clientX - _this3.touchStartX;[m
[32m+[m[32m        }[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      var end = function end(event) {[m
[32m+[m[32m        if (_this3._pointerEvent && PointerType[event.originalEvent.pointerType.toUpperCase()]) {[m
[32m+[m[32m          _this3.touchDeltaX = event.originalEvent.clientX - _this3.touchStartX;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        _this3._handleSwipe();[m
[32m+[m
[32m+[m[32m        if (_this3._config.pause === 'hover') {[m
[32m+[m[32m          // If it's a touch-enabled device, mouseenter/leave are fired as[m
[32m+[m[32m          // part of the mouse compatibility events on first tap - the carousel[m
[32m+[m[32m          // would stop cycling until user tapped out of it;[m
[32m+[m[32m          // here, we listen for touchend, explicitly pause the carousel[m
[32m+[m[32m          // (as if it's the second time we tap on it, mouseenter compat event[m
[32m+[m[32m          // is NOT fired) and after a timeout (to allow for mouse compatibility[m
[32m+[m[32m          // events to fire) we explicitly restart cycling[m
[32m+[m[32m          _this3.pause();[m
[32m+[m
[32m+[m[32m          if (_this3.touchTimeout) {[m
[32m+[m[32m            clearTimeout(_this3.touchTimeout);[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          _this3.touchTimeout = setTimeout(function (event) {[m
[32m+[m[32m            return _this3.cycle(event);[m
[32m+[m[32m          }, TOUCHEVENT_COMPAT_WAIT + _this3._config.interval);[m
[32m+[m[32m        }[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      $__default['default'](this._element.querySelectorAll(SELECTOR_ITEM_IMG)).on(EVENT_DRAG_START, function (e) {[m
[32m+[m[32m        return e.preventDefault();[m
[32m+[m[32m      });[m
[32m+[m
[32m+[m[32m      if (this._pointerEvent) {[m
[32m+[m[32m        $__default['default'](this._element).on(EVENT_POINTERDOWN, function (event) {[m
[32m+[m[32m          return start(event);[m
[32m+[m[32m        });[m
[32m+[m[32m        $__default['default'](this._element).on(EVENT_POINTERUP, function (event) {[m
[32m+[m[32m          return end(event);[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        this._element.classList.add(CLASS_NAME_POINTER_EVENT);[m
[32m+[m[32m      } else {[m
[32m+[m[32m        $__default['default'](this._element).on(EVENT_TOUCHSTART, function (event) {[m
[32m+[m[32m          return start(event);[m
[32m+[m[32m        });[m
[32m+[m[32m        $__default['default'](this._element).on(EVENT_TOUCHMOVE, function (event) {[m
[32m+[m[32m          return move(event);[m
[32m+[m[32m        });[m
[32m+[m[32m        $__default['default'](this._element).on(EVENT_TOUCHEND, function (event) {[m
[32m+[m[32m          return end(event);[m
[32m+[m[32m        });[m
[32m+[m[32m      }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    _proto._keydown = function _keydown(event) {[m
[32m+[m[32m      if (/input|textarea/i.test(event.target.tagName)) {[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      switch (event.which) {[m
[32m+[m[32m        case ARROW_LEFT_KEYCODE:[m
[32m+[m[32m          event.preventDefault();[m
[32m+[m[32m          this.prev();[m
[32m+[m[32m          break;[m
[32m+[m
[32m+[m[32m        case ARROW_RIGHT_KEYCODE:[m
[32m+[m[32m          event.preventDefault();[m
[32m+[m[32m          this.next();[m
[32m+[m[32m          break;[m
[32m+[m[32m      }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    _proto._getItemIndex = function _getItemIndex(element) {[m
[32m+[m[32m      this._items = element && element.parentNode ? [].slice.call(element.parentNode.querySelectorAll(SELECTOR_ITEM)) : [];[m
[32m+[m[32m      return this._items.indexOf(element);[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    _proto._getItemByDirection = function _getItemByDirection(direction, activeElement) {[m
[32m+[m[32m      var isNextDirection = direction === DIRECTION_NEXT;[m
[32m+[m[32m      var isPrevDirection = direction === DIRECTION_PREV;[m
[32m+[m
[32m+[m[32m      var activeIndex = this._getItemIndex(activeElement);[m
[32m+[m
[32m+[m[32m      var lastItemIndex = this._items.length - 1;[m
[32m+[m[32m      var isGoingToWrap = isPrevDirection && activeIndex === 0 || isNextDirection && activeIndex === lastItemIndex;[m
[32m+[m
[32m+[m[32m      if (isGoingToWrap && !this._config.wrap) {[m
[32m+[m[32m        return activeElement;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      var delta = direction === DIRECTION_PREV ? -1 : 1;[m
[32m+[m[32m      var itemIndex = (activeIndex + delta) % this._items.length;[m
[32m+[m[32m      return itemIndex === -1 ? this._items[this._items.length - 1] : this._items[itemIndex];[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    _proto._triggerSlideEvent = function _triggerSlideEvent(relatedTarget, eventDirectionName) {[m
[32m+[m[32m      var targetIndex = this._getItemIndex(relatedTarget);[m
[32m+[m
[32m+[m[32m      var fromIndex = this._getItemIndex(this._element.querySelector(SELECTOR_ACTIVE_ITEM));[m
[32m+[m
[32m+[m[32m      var slideEvent = $__default['default'].Event(EVENT_SLIDE, {[m
[32m+[m[32m        relatedTarget: relatedTarget,[m
[32m+[m[32m        direction: eventDirectionName,[m
[32m+[m[32m        from: fromIndex,[m
[32m+[m[32m        to: targetIndex[m
[32m+[m[32m      });[m
[32m+[m[32m      $__default['default'](this._element).trigger(slideEvent);[m
[32m+[m[32m      return slideEvent;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    _proto._setActiveIndicatorElement = function _setActiveIndicatorElement(element) {[m
[32m+[m[32m      if (this._indicatorsElement) {[m
[32m+[m[32m        var indicators = [].slice.call(this._indicatorsElement.querySelectorAll(SELECTOR_ACTIVE$1));[m
[32m+[m[32m        $__default['default'](indicators).removeClass(CLASS_NAME_ACTIVE$1);[m
[32m+[m
[32m+[m[32m        var nextIndicator = this._indicatorsElement.children[this._getItemIndex(element)];[m
[32m+[m
[32m+[m[32m        if (nextIndicator) {[m
[32m+[m[32m          $__default['default'](nextIndicator).addClass(CLASS_NAME_ACTIVE$1);[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    _proto._updateInterval = function _updateInterval() {[m
[32m+[m[32m      var element = this._activeElement || this._element.querySelector(SELECTOR_ACTIVE_ITEM);[m
[32m+[m
[32m+[m[32m      if (!element) {[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      var elementInterval = parseInt(element.getAttribute('data-interval'), 10);[m
[32m+[m
[32m+[m[32m      if (elementInterval) {[m
[32m+[m[32m        this._config.defaultInterval = this._config.defaultInterval || this._config.interval;[m
[32m+[m[32m        this._config.interval = elementInterval;[m
[32m+[m[32m      } else {[m
[32m+[m[32m        this._config.interval = this._config.defaultInterval || this._config.interval;[m
[32m+[m[32m      }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    _proto._slide = function _slide(direction, element) {[m
[32m+[m[32m      var _this4 = this;[m
[32m+[m
[32m+[m[32m      var activeElement = this._element.querySelector(SELECTOR_ACTIVE_ITEM);[m
[32m+[m
[32m+[m[32m      var activeElementIndex = this._getItemIndex(activeElement);[m
[32m+[m
[32m+[m[32m      var nextElement = element || activeElement && this._getItemByDirection(direction, activeElement);[m
[32m+[m
[32m+[m[32m      var nextElementIndex = this._getItemIndex(nextElement);[m
[32m+[m
[32m+[m[32m      var isCycling = Boolean(this._interval);[m
[32m+[m[32m      var directionalClassName;[m
[32m+[m[32m      var orderClassName;[m
[32m+[m[32m      var eventDirectionName;[m
[32m+[m
[32m+[m[32m      if (direction === DIRECTION_NEXT) {[m
[32m+[m[32m        directionalClassName = CLASS_NAME_LEFT;[m
[32m+[m[32m        orderClassName = CLASS_NAME_NEXT;[m
[32m+[m[32m        eventDirectionName = DIRECTION_LEFT;[m
[32m+[m[32m      } else {[m
[32m+[m[32m        directionalClassName = CLASS_NAME_RIGHT;[m
[32m+[m[32m        orderClassName = CLASS_NAME_PREV;[m
[32m+[m[32m        eventDirectionName = DIRECTION_RIGHT;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (nextElement && $__default['default'](nextElement).hasClass(CLASS_NAME_ACTIVE$1)) {[m
[32m+[m[32m        this._isSliding = false;[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      var slideEvent = this._triggerSlideEvent(nextElement, eventDirectionName);[m
[32m+[m
[32m+[m[32m      if (slideEvent.isDefaultPrevented()) {[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (!activeElement || !nextElement) {[m
[32m+[m[32m        // Some weirdness is happening, so we bail[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      this._isSliding = true;[m
[32m+[m
[32m+[m[32m      if (isCycling) {[m
[32m+[m[32m        this.pause();[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      this._setActiveIndicatorElement(nextElement);[m
[32m+[m
[32m+[m[32m      this._activeElement = nextElement;[m
[32m+[m[32m      var slidEvent = $__default['default'].Event(EVENT_SLID, {[m
[32m+[m[32m        relatedTarget: nextElement,[m
[32m+[m[32m        direction: eventDirectionName,[m
[32m+[m[32m        from: activeElementIndex,[m
[32m+[m[32m        to: nextElementIndex[m
[32m+[m[32m      });[m
[32m+[m
[32m+[m[32m      if ($__default['default'](this._element).hasClass(CLASS_NAME_SLIDE)) {[m
[32m+[m[32m        $__default['default'](nextElement).addClass(orderClassName);[m
[32m+[m[32m        Util.reflow(nextElement);[m
[32m+[m[32m        $__default['default'](activeElement).addClass(directionalClassName);[m
[32m+[m[32m        $__default['default'](nextElement).addClass(directionalClassName);[m
[32m+[m[32m        var transitionDuration = Util.getTransitionDurationFromElement(activeElement);[m
[32m+[m[32m        $__default['default'](activeElement).one(Util.TRANSITION_END, function () {[m
[32m+[m[32m          $__default['default'](nextElement).removeClass(directionalClassName + " " + orderClassName).addClass(CLASS_NAME_ACTIVE$1);[m
[32m+[m[32m          $__default['default'](activeElement).removeClass(CLASS_NAME_ACTIVE$1 + " " + orderClassName + " " + directionalClassName);[m
[32m+[m[32m          _this4._isSliding = false;[m
[32m+[m[32m          setTimeout(function () {[m
[32m+[m[32m            return $__default['default'](_this4._element).trigger(slidEvent);[m
[32m+[m[32m          }, 0);[m
[32m+[m[32m        }).emulateTransitionEnd(transitionDuration);[m
[32m+[m[32m      } else {[m
[32m+[m[32m        $__default['default'](activeElement).removeClass(CLASS_NAME_ACTIVE$1);[m
[32m+[m[32m        $__default['default'](nextElement).addClass(CLASS_NAME_ACTIVE$1);[m
[32m+[m[32m        this._isSliding = false;[m
[32m+[m[32m        $__default['default'](this._element).trigger(slidEvent);[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (isCycling) {[m
[32m+[m[32m        this.cycle();[m
[32m+[m[32m      }[m
[32m+[m[32m    } // Static[m
[32m+[m[32m    ;[m
[32m+[m
[32m+[m[32m    Carousel._jQueryInterface = function _jQueryInterface(config) {[m
[32m+[m[32m      return this.each(function () {[m
[32m+[m[32m        var data = $__default['default'](this).data(DATA_KEY$2);[m
[32m+[m
[32m+[m[32m        var _config = _extends({}, Default, $__default['default'](this).data());[m
[32m+[m
[32m+[m[32m        if (typeof config === 'object') {[m
[32m+[m[32m          _config = _extends({}, _config, config);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        var action = typeof config === 'string' ? config : _config.slide;[m
[32m+[m
[32m+[m[32m        if (!data) {[m
[32m+[m[32m          data = new Carousel(this, _config);[m
[32m+[m[32m          $__default['default'](this).data(DATA_KEY$2, data);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (typeof config === 'number') {[m
[32m+[m[32m          data.to(config);[m
[32m+[m[32m        } else if (typeof action === 'string') {[m
[32m+[m[32m          if (typeof data[action] === 'undefined') {[m
[32m+[m[32m            throw new TypeError("No method named \"" + action + "\"");[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          data[action]();[m
[32m+[m[32m        } else if (_config.interval && _config.ride) {[m
[32m+[m[32m          data.pause();[m
[32m+[m[32m          data.cycle();[m
[32m+[m[32m        }[m
[32m+[m[32m      });[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    Carousel._dataApiClickHandler = function _dataApiClickHandler(event) {[m
[32m+[m[32m      var selector = Util.getSelectorFromElement(this);[m
[32m+[m
[32m+[m[32m      if (!selector) {[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      var target = $__default['default'](selector)[0];[m
[32m+[m
[32m+[m[32m      if (!target || !$__default['default'](target).hasClass(CLASS_NAME_CAROUSEL)) {[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
