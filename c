[32m+[m[32m  $__default['default'](document).on(EVENT_CLICK_DATA_API$5, SELECTOR_DATA_TOGGLE$3, function (event) {[m
[32m+[m[32m    var _this11 = this;[m
[32m+[m
[32m+[m[32m    var target;[m
[32m+[m[32m    var selector = Util.getSelectorFromElement(this);[m
[32m+[m
[32m+[m[32m    if (selector) {[m
[32m+[m[32m      target = document.querySelector(selector);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var config = $__default['default'](target).data(DATA_KEY$5) ? 'toggle' : _extends({}, $__default['default'](target).data(), $__default['default'](this).data());[m
[32m+[m
[32m+[m[32m    if (this.tagName === 'A' || this.tagName === 'AREA') {[m
[32m+[m[32m      event.preventDefault();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var $target = $__default['default'](target).one(EVENT_SHOW$2, function (showEvent) {[m
[32m+[m[32m      if (showEvent.isDefaultPrevented()) {[m
[32m+[m[32m        // Only register focus restorer if modal will actually get shown[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      $target.one(EVENT_HIDDEN$2, function () {[m
[32m+[m[32m        if ($__default['default'](_this11).is(':visible')) {[m
[32m+[m[32m          _this11.focus();[m
[32m+[m[32m        }[m
[32m+[m[32m      });[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    Modal._jQueryInterface.call($__default['default'](target), config, this);[m
[32m+[m[32m  });[m
