[32m+[m[32m    _proto._enter = function _enter(event, context) {[m
[32m+[m[32m      var dataKey = this.constructor.DATA_KEY;[m
[32m+[m[32m      context = context || $__default['default'](event.currentTarget).data(dataKey);[m
