[32m+[m[32m(function($) {[m
[32m+[m[32m  "use strict"; // Start of use strict[m
[32m+[m
[32m+[m[32m  // Toggle the side navigation[m
[32m+[m[32m  $("#sidebarToggle, #sidebarToggleTop").on('click', function(e) {[m
[32m+[m[32m    $("body").toggleClass("sidebar-toggled");[m
[32m+[m[32m    $(".sidebar").toggleClass("toggled");[m
[32m+[m[32m    if ($(".sidebar").hasClass("toggled")) {[m
[32m+[m[32m      $('.sidebar .collapse').collapse('hide');[m
[32m+[m[32m    };[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m  // Close any open menu accordions when window is resized below 768px[m
[32m+[m[32m  $(window).resize(function() {[m
[32m+[m[32m    if ($(window).width() < 768) {[m
[32m+[m[32m      $('.sidebar .collapse').collapse('hide');[m
[32m+[m[32m    };[m
[32m+[m[41m    [m
[32m+[m[32m    // Toggle the side navigation when window is resized below 480px[m
[32m+[m[32m    if ($(window).width() < 480 && !$(".sidebar").hasClass("toggled")) {[m
[32m+[m[32m      $("body").addClass("sidebar-toggled");[m
[32m+[m[32m      $(".sidebar").addClass("toggled");[m
[32m+[m[32m      $('.sidebar .collapse').collapse('hide');[m
[32m+[m[32m    };[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m  // Prevent the content wrapper from scrolling when the fixed side navigation hovered over[m
[32m+[m[32m  $('body.fixed-nav .sidebar').on('mousewheel DOMMouseScroll wheel', function(e) {[m
[32m+[m[32m    if ($(window).width() > 768) {[m
[32m+[m[32m      var e0 = e.originalEvent,[m
[32m+[m[32m        delta = e0.wheelDelta || -e0.detail;[m
[32m+[m[32m      this.scrollTop += (delta < 0 ? 1 : -1) * 30;[m
[32m+[m[32m      e.preventDefault();[m
[32m+[m[32m    }[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m  // Scroll to top button appear[m
[32m+[m[32m  $(document).on('scroll', function() {[m
[32m+[m[32m    var scrollDistance = $(this).scrollTop();[m
[32m+[m[32m    if (scrollDistance > 100) {[m
[32m+[m[32m      $('.scroll-to-top').fadeIn();[m
[32m+[m[32m    } else {[m
[32m+[m[32m      $('.scroll-to-top').fadeOut();[m
[32m+[m[32m    }[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m  // Smooth scrolling using jQuery easing[m
[32m+[m[32m  $(document).on('click', 'a.scroll-to-top', function(e) {[m
[32m+[m[32m    var $anchor = $(this);[m
[32m+[m[32m    $('html, body').stop().animate({[m
[32m+[m[32m      scrollTop: ($($anchor.attr('href')).offset().top)[m
[32m+[m[32m    }, 1000, 'easeInOutExpo');[m
[32m+[m[32m    e.preventDefault();[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m})(jQuery); // End of use strict[m
