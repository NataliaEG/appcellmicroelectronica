[32m+[m[32mfunction borrarCliente(event) {[m
[32m+[m[32m  const button = event.target;[m
[32m+[m[32m  // Obtener el ID del cliente del atributo "data-id" del botón[m
[32m+[m[32m  const numero = button.getAttribute("data-id");[m
[32m+[m
[32m+[m[32m  // Confirmar la eliminación con el usuario[m
[32m+[m[32m  //if (confirm("¿Estás seguro de que deseas eliminar este cliente?")) {[m
[32m+[m[32m  // }[m
[32m+[m[32m  Swal[m
[32m+[m[32m    .fire({[m
[32m+[m[32m        title: "¿Eliminar cliente?",[m
[32m+[m[32m        text: "¿Estás seguro de que deseas eliminar este cliente?",[m
[32m+[m[32m        icon: 'question',[m
[32m+[m[32m        showCancelButton: true,[m
[32m+[m[32m        confirmButtonText: "Sí, eliminar",[m
[32m+[m[32m        cancelButtonText: "Cancelar",[m
[32m+[m[32m    })[m
[32m+[m[32m    .then(resultado => {[m
[32m+[m[32m      if (resultado.value) {[m
[32m+[m[41m      [m
[32m+[m[32m    // Enviar una solicitud fetch para eliminar el cliente[m
[32m+[m[32m    fetch('eliminar_cliente.php', {[m
[32m+[m[32m      method: 'POST',[m
[32m+[m[32m      headers: {[m
[32m+[m[32m        'Content-Type': 'application/json'[m
[32m+[m[32m      },[m
[32m+[m[32m      body: JSON.stringify({ numero: numero })[m
[32m+[m[32m    })[m
[32m+[m[32m      .then(response => response.json())[m
[32m+[m[32m      .then(data => {[m
[32m+[m[32m        // Hacer algo con la respuesta del servidor, si es necesario[m
[32m+[m[32m        console.log(data);[m
[32m+[m
[32m+[m[32m        // Mostrar una alerta si el cliente se eliminó correctamente[m
[32m+[m[32m        if (data.success) {[m
[32m+[m[32m          lanzarAlerta("Cliente eliminado", "El cliente ha sido eliminado correctamente", "success");[m
[32m+[m[32m          obtenerLista(); // Actualizar la lista de clientes después de la eliminación[m
[32m+[m[32m        } else {[m
[32m+[m[32m          lanzarAlerta("Error", "No se pudo eliminar el cliente", "error");[m
[32m+[m[32m        }[m
[32m+[m[32m      })[m
[32m+[m[32m      .catch(error => {[m
[32m+[m[32m        console.error(error);[m
[32m+[m[32m        lanzarAlerta("Error", "Ocurrió un error al procesar la solicitud", "error");[m
[32m+[m[32m      });[m
[32m+[m[32m        // Hicieron click en "Sí"[m
[32m+[m[32m        console.log("*se elimina la venta*");[m
[32m+[m[32m      } else {[m
[32m+[m[32m          // Dijeron que no[m
[32m+[m[32m          console.log("*NO se elimina la venta*");[m
[32m+[m[32m      }[m
[32m+[m[32m  });[m
[32m+[m[41m  [m
[32m+[m[32m}[m
