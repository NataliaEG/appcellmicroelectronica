[32m+[m
[32m+[m[32mfunction cargarDatos(arrayClientes) {[m
[32m+[m[32m  let contenedor = document.getElementById("datosClientes");[m
[32m+[m[32m  contenedor.innerHTML = "";[m
[32m+[m
[32m+[m[32m  arrayClientes.forEach([m
[32m+[m[32m    ({[m
[32m+[m[32m      numero,[m
[32m+[m[32m      cliente,[m
[32m+[m[32m      telefono,[m
[32m+[m[32m      codigo,[m
[32m+[m[32m      modelo,[m
[32m+[m[32m      falla,[m
[32m+[m[32m      observacion,[m
[32m+[m[32m      fecha_ingreso,[m
[32m+[m[32m      fecha_entrega,[m
[32m+[m[32m      precio,[m
[32m+[m[32m      imei,[m
[32m+[m[32m      estado,[m
[32m+[m[32m    }) => {[m
[32m+[m[32m      let tarjeta = document.createElement("tr");[m
[32m+[m[32m      tarjeta.className = "tarjeta";[m
[32m+[m
[32m+[m[32m      tarjeta.innerHTML = `[m
[32m+[m[32m        <td scope="row">${numero}</td>[m
[32m+[m[32m        <td scope="row">${cliente}</td>[m
[32m+[m[32m        <td scope="row">${telefono}</td>[m
[32m+[m[32m        <td scope="row">${codigo}</td>[m
[32m+[m[32m        <td scope="row">${modelo}</td>[m
[32m+[m[32m        <td scope="row">${falla}</td>[m
[32m+[m[32m        <td scope="row">${observacion}</td>[m
[32m+[m[32m        <td scope="row">${fecha_ingreso}</td>[m
[32m+[m[32m        <td scope="row">${fecha_entrega}</td>[m
[32m+[m[32m        <td scope="row">${precio}</td>[m
[32m+[m[32m        <td scope="row">${imei}</td>[m
[32m+[m[32m        <td scope="row">${estado}</td>[m
[32m+[m[32m        <td>[m
[32m+[m[32m          <a class="btn btn-primary" onclick="editarModal(${numero})" value="${numero}">[m
[32m+[m[32m            <i class="fa fa-edit"></i>[m
[32m+[m[32m          </a>[m
[32m+[m[32m        </td>[m
[32m+[m[32m        <td>[m
[32m+[m[32m          <button  class="btn btn-danger" id="borrar"  onclick="borrarCliente(event)" data-id="${numero}">[m
[32m+[m[32m            <i class="fa fa-trash"></i>[m
[32m+[m[32m          </button>[m
[32m+[m[32m        </td>[m
[32m+[m[32m      `;[m
[32m+[m
[32m+[m[32m      contenedor.appendChild(tarjeta);[m
[32m+[m[32m    }[m
[32m+[m[32m  );[m
[32m+[m[32m}[m
