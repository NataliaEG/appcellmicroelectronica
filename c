[32m+[m[32m<?php[m
[32m+[m[32minclude("../conexion.php");[m
[32m+[m
[32m+[m[32m$method = $_SERVER["REQUEST_METHOD"];[m
[32m+[m
[32m+[m[32mswitch ($method) {[m
[32m+[m[32m    case 'GET':[m
[32m+[m[32m        if (!isset($_GET['numero'])) {[m
[32m+[m[32m            // Obtener todos los registros[m
[32m+[m[32m            $result = $con->query("SELECT * FROM trabajos");[m
[32m+[m
[32m+[m[32m            // Crear un array para almacenar los registros[m
[32m+[m[32m            $elementos = array();[m
[32m+[m
[32m+[m[32m            // Iterar sobre cada fila de resultados y almacenar los campos en el array[m
[32m+[m[32m            while ($row = $result->fetch_assoc()) {[m
[32m+[m[32m                $elementos[] = $row;[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            // Devolver los campos en formato JSON[m
[32m+[m[32m            echo json_encode($elementos);[m
[32m+[m[32m        } else {[m
[32m+[m[32m            // Obtener un registro en particular[m
[32m+[m[32m            $numero = intval($_GET['numero']);[m
[32m+[m[32m            $result = $con->query("SELECT * FROM trabajos WHERE numero= $numero");[m
[32m+[m
[32m+[m[32m            // Crear un array para almacenar el registro[m
[32m+[m[32m            $elementos = array();[m
[32m+[m
[32m+[m[32m            // Iterar sobre cada fila de resultados y almacenar los campos en el array[m
[32m+[m[32m            while ($row = $result->fetch_assoc()) {[m
[32m+[m[32m                $elementos[] = $row;[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            // Devolver los campos en formato JSON[m
[32m+[m[32m            echo json_encode($elementos);[m
[32m+[m[32m        }[m
[32m+[m[32m        break;[m
[32m+[m
[32m+[m[32m    case 'POST':[m
[32m+[m[32m        // Leer el cuerpo de la solicitud y decodificarlo como JSON[m
[32m+[m[32m        $input = json_decode(file_get_contents('php://input'), true);[m
[32m+[m
[32m+[m[32m        $numero = $input['numero'];[m
[32m+[m[32m        $cliente = $input['cliente'];[m
[32m+[m[32m        $telefono = $input['telefono'];[m
[32m+[m[32m        $codigo = $input['codigo'];[m
[32m+[m[32m        $modelo = $input['modelo'];[m
[32m+[m[32m        $falla = $input['falla'];[m
[32m+[m[32m        $observacion = $input['observacion'];[m
[32m+[m[32m        $fecha_ingreso = $input['fecha_ingreso'];[m
[32m+[m[32m        $fecha_entrega = $input['fecha_entrega'];[m
[32m+[m[32m        $precio = $input['precio'];[m
[32m+[m[32m        $imei = $input['imei'];[m
[32m+[m[32m        $estado = $input['estado'];[m
[32m+[m
[32m+[m[32m        // Actualizar registros[m
[32m+[m[32m        $sql = "UPDATE trabajos SET cliente=?, telefono=?, codigo=?, modelo=?, falla=?, observacion=?, fecha_ingreso=?, fecha_entrega=?, precio=?, imei=?, estado=? WHERE numero=?";[m
[32m+[m[32m        $query = $con->prepare($sql);[m
[32m+[m[32m        $query->bind_param("sssssssssssi", $cliente, $telefono, $codigo, $modelo, $falla, $observacion, $fecha_ingreso, $fecha_entrega, $precio, $imei, $estado, $numero);[m
[32m+[m[32m        $query->execute();[m
[32m+[m
[32m+[m[32m        // Devolver un código de estado de éxito[m
[32m+[m[32m        $data = array("estado" => true);[m
[32m+[m[32m        echo json_encode($data);[m
[32m+[m[32m        break;[m
[32m+[m
[32m+[m[32m    default:[m
[32m+[m[32m        // Manejar otros métodos HTTP si es necesario[m
[32m+[m[32m        break;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mmysqli_close($con);[m
[32m+[m[32m?>[m
