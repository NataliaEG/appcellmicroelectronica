[32m+[m
[32m+[m[32m//---------------Filtrar estado--------------------------[m
[32m+[m
[32m+[m[32mlet selectEstado = document.getElementById("estado");[m
[32m+[m
[32m+[m[32mselectEstado.addEventListener("change", filtrarEstado);[m
[32m+[m
[32m+[m[32mfunction filtrarEstado() {[m
[32m+[m[32m  let estadoSeleccionado = selectEstado.value;[m
[32m+[m[41m  [m
[32m+[m[32m  if (estadoSeleccionado !== "") {[m
[32m+[m[32m    let arrayFiltrado = obj.filter(ob => ob.estado === estadoSeleccionado);[m
[32m+[m[32m    cargarDatos(arrayFiltrado);[m
[32m+[m[32m  } else {[m
[32m+[m[32m    cargarDatos(obj);[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m//-------------Paginacion-----------------[m
[32m+[m[32mfunction cargarPagina(page, pageSize) {[m
[32m+[m[32m  const url = `paginacion.php?page=${page}&pageSize=${pageSize}`;[m
[32m+[m
[32m+[m[32m  fetch(url)[m
[32m+[m[32m    .then(response => response.json())[m
[32m+[m[32m    .then(data => {[m
[32m+[m[32m      // En este punto, 'data' contiene los datos de la página solicitada[m
[32m+[m[32m      // Aquí debes actualizar la tabla con los nuevos datos recibidos[m
[32m+[m[32m      cargarDatos(data.data);[m
[32m+[m
[32m+[m[32m      // Luego, actualizamos los controles de paginación con los datos recibidos[m
[32m+[m[32m      actualizarControlesPaginacion(page, pageSize, data.total);[m
[32m+[m[32m    })[m
[32m+[m[32m    .catch(error => {[m
[32m+[m[32m      // Manejo de errores si la solicitud falla[m
[32m+[m[32m      console.error('Error al cargar la página:', error);[m
[32m+[m[32m    });[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Llamada inicial para cargar la primera página[m
[32m+[m[32mcargarPagina(1, 5); // Puedes ajustar los valores de página y pageSize según tus necesidades[m
[32m+[m
[32m+[m[32m// Función para actualizar los controles de paginación[m
[32m+[m[32mfunction actualizarControlesPaginacion(page, pageSize, total) {[m
[32m+[m[32m  const anteriorElement = document.getElementById('anterior');[m
[32m+[m[32m  const siguienteElement = document.getElementById('siguiente');[m
[32m+[m[32m  const numPaginaElement = document.getElementById('numPagina');[m
[32m+[m
[32m+[m[32m  // Actualizar el número de página actual y el total de páginas[m
[32m+[m[32m  numPaginaElement.textContent = `Página ${page} de ${Math.ceil(total / pageSize)}`;[m
[32m+[m
[32m+[m[32m  // Habilitar o deshabilitar el enlace de página anterior según corresponda[m
[32m+[m[32m  if (page === 1) {[m
[32m+[m[32m    anteriorElement.classList.add('disabled');[m
[32m+[m[32m  } else {[m
[32m+[m[32m    anteriorElement.classList.remove('disabled');[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Habilitar o deshabilitar el enlace de página siguiente según corresponda[m
[32m+[m[32m  if (page === Math.ceil(total / pageSize)) {[m
[32m+[m[32m    siguienteElement.classList.add('disabled');[m
[32m+[m[32m  } else {[m
[32m+[m[32m    siguienteElement.classList.remove('disabled');[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Asignar los manejadores de eventos a los enlaces de página anterior y siguiente[m
[32m+[m[32m  anteriorElement.addEventListener('click', () => {[m
[32m+[m[32m    if (page > 1) {[m
[32m+[m[32m      cargarPagina(page - 1, pageSize);[m
[32m+[m[32m    }[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m  siguienteElement.addEventListener('click', () => {[m
[32m+[m[32m    if (page < Math.ceil(total / pageSize)) {[m
[32m+[m[32m      cargarPagina(page + 1, pageSize);[m
[32m+[m[32m    }[m
[32m+[m[32m  });[m
[32m+[m[32m}[m
[32m+[m[32m//-------------Fin Paginacion-----------------[m
[32m+[m
[32m+[m[32m//-------------Inicio generado de codigo-----------------[m
[32m+[m[32mlet numeros= "0123456789"[m
[32m+[m[32mlet letras= "abcdefghijklmnopqrstuvwxyz"[m
[32m+[m[32mlet todo= numeros + letras[m
[32m+[m
[32m+[m[32mfunction generateRandomNumber() {[m
[32m+[m[32m  let longitud= 6[m
[32m+[m[32m  let password= ""[m
[32m+[m[32m  for(let x = 0; x<longitud; x++){[m
[32m+[m[32m    let aleatorio= Math.floor(Math.random() * todo.length)[m
[32m+[m[32m    password += todo.charAt(aleatorio)[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // Asigna el número aleatorio al input[m
[32m+[m[32m  document.getElementById('modal-codigo').value = password;[m
[32m+[m[32m}[m
[32m+[m[32m//-------------Fin generado de codigo-----------------[m
\ No newline at end of file[m
