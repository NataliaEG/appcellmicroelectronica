[32m+[m[32m  $__default['default'](window).on(EVENT_LOAD_DATA_API, function () {[m
[32m+[m[32m    // ensure correct active class is set to match the controls' actual values/states[m
[32m+[m[32m    // find all checkboxes/readio buttons inside data-toggle groups[m
[32m+[m[32m    var buttons = [].slice.call(document.querySelectorAll(SELECTOR_DATA_TOGGLES_BUTTONS));[m
[32m+[m
[32m+[m[32m    for (var i = 0, len = buttons.length; i < len; i++) {[m
