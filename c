[32m+[m[32m      var shownEvent = $__default['default'].Event(EVENT_SHOWN$2, {[m
[32m+[m[32m        relatedTarget: relatedTarget[m
[32m+[m[32m      });[m
[32m+[m
[32m+[m[32m      var transitionComplete = function transitionComplete() {[m
[32m+[m[32m        if (_this4._config.focus) {[m
[32m+[m[32m          _this4._element.focus();[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        _this4._isTransitioning = false;[m
[32m+[m[32m        $__default['default'](_this4._element).trigger(shownEvent);[m
[32m+[m[32m      };[m
[32m+[m
[32m+[m[32m      if (transition) {[m
[32m+[m[32m        var transitionDuration = Util.getTransitionDurationFromElement(this._dialog);[m
[32m+[m[32m        $__default['default'](this._dialog).one(Util.TRANSITION_END, transitionComplete).emulateTransitionEnd(transitionDuration);[m
[32m+[m[32m      } else {[m
[32m+[m[32m        transitionComplete();[m
[32m+[m[32m      }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    _proto._enforceFocus = function _enforceFocus() {[m
[32m+[m[32m      var _this5 = this;[m
[32m+[m
[32m+[m[32m      $__default['default'](document).off(EVENT_FOCUSIN) // Guard against infinite focus loop[m
[32m+[m[32m      .on(EVENT_FOCUSIN, function (event) {[m
[32m+[m[32m        if (document !== event.target && _this5._element !== event.target && $__default['default'](_this5._element).has(event.target).length === 0) {[m
[32m+[m[32m          _this5._element.focus();[m
[32m+[m[32m        }[m
[32m+[m[32m      });[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    _proto._setEscapeEvent = function _setEscapeEvent() {[m
[32m+[m[32m      var _this6 = this;[m
[32m+[m
[32m+[m[32m      if (this._isShown) {[m
[32m+[m[32m        $__default['default'](this._element).on(EVENT_KEYDOWN_DISMISS, function (event) {[m
[32m+[m[32m          if (_this6._config.keyboard && event.which === ESCAPE_KEYCODE$1) {[m
[32m+[m[32m            event.preventDefault();[m
[32m+[m
[32m+[m[32m            _this6.hide();[m
[32m+[m[32m          } else if (!_this6._config.keyboard && event.which === ESCAPE_KEYCODE$1) {[m
[32m+[m[32m            _this6._triggerBackdropTransition();[m
[32m+[m[32m          }[m
[32m+[m[32m        });[m
[32m+[m[32m      } else if (!this._isShown) {[m
[32m+[m[32m        $__default['default'](this._element).off(EVENT_KEYDOWN_DISMISS);[m
[32m+[m[32m      }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    _proto._setResizeEvent = function _setResizeEvent() {[m
[32m+[m[32m      var _this7 = this;[m
[32m+[m
[32m+[m[32m      if (this._isShown) {[m
[32m+[m[32m        $__default['default'](window).on(EVENT_RESIZE, function (event) {[m
[32m+[m[32m          return _this7.handleUpdate(event);[m
[32m+[m[32m        });[m
[32m+[m[32m      } else {[m
[32m+[m[32m        $__default['default'](window).off(EVENT_RESIZE);[m
[32m+[m[32m      }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    _proto._hideModal = function _hideModal() {[m
[32m+[m[32m      var _this8 = this;[m
[32m+[m
[32m+[m[32m      this._element.style.display = 'none';[m
[32m+[m
[32m+[m[32m      this._element.setAttribute('aria-hidden', true);[m
[32m+[m
[32m+[m[32m      this._element.removeAttribute('aria-modal');[m
[32m+[m
[32m+[m[32m      this._element.removeAttribute('role');[m
[32m+[m
[32m+[m[32m      this._isTransitioning = false;[m
[32m+[m
[32m+[m[32m      this._showBackdrop(function () {[m
[32m+[m[32m        $__default['default'](document.body).removeClass(CLASS_NAME_OPEN);[m
[32m+[m
[32m+[m[32m        _this8._resetAdjustments();[m
[32m+[m
[32m+[m[32m        _this8._resetScrollbar();[m
[32m+[m
[32m+[m[32m        $__default['default'](_this8._element).trigger(EVENT_HIDDEN$2);[m
[32m+[m[32m      });[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    _proto._removeBackdrop = function _removeBackdrop() {[m
[32m+[m[32m      if (this._backdrop) {[m
[32m+[m[32m        $__default['default'](this._backdrop).remove();[m
[32m+[m[32m        this._backdrop = null;[m
[32m+[m[32m      }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    _proto._showBackdrop = function _showBackdrop(callback) {[m
[32m+[m[32m      var _this9 = this;[m
[32m+[m
[32m+[m[32m      var animate = $__default['default'](this._element).hasClass(CLASS_NAME_FADE$1) ? CLASS_NAME_FADE$1 : '';[m
[32m+[m
[32m+[m[32m      if (this._isShown && this._config.backdrop) {[m
[32m+[m[32m        this._backdrop = document.createElement('div');[m
[32m+[m[32m        this._backdrop.className = CLASS_NAME_BACKDROP;[m
[32m+[m
[32m+[m[32m        if (animate) {[m
[32m+[m[32m          this._backdrop.classList.add(animate);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        $__default['default'](this._backdrop).appendTo(document.body);[m
[32m+[m[32m        $__default['default'](this._element).on(EVENT_CLICK_DISMISS, function (event) {[m
[32m+[m[32m          if (_this9._ignoreBackdropClick) {[m
[32m+[m[32m            _this9._ignoreBackdropClick = false;[m
[32m+[m[32m            return;[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          if (event.target !== event.currentTarget) {[m
[32m+[m[32m            return;[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          if (_this9._config.backdrop === 'static') {[m
[32m+[m[32m            _this9._triggerBackdropTransition();[m
[32m+[m[32m          } else {[m
[32m+[m[32m            _this9.hide();[m
[32m+[m[32m          }[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        if (animate) {[m
[32m+[m[32m          Util.reflow(this._backdrop);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        $__default['default'](this._backdrop).addClass(CLASS_NAME_SHOW$3);[m
[32m+[m
[32m+[m[32m        if (!callback) {[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (!animate) {[m
[32m+[m[32m          callback();[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        var backdropTransitionDuration = Util.getTransitionDurationFromElement(this._backdrop);[m
[32m+[m[32m        $__default['default'](this._backdrop).one(Util.TRANSITION_END, callback).emulateTransitionEnd(backdropTransitionDuration);[m
[32m+[m[32m      } else if (!this._isShown && this._backdrop) {[m
[32m+[m[32m        $__default['default'](this._backdrop).removeClass(CLASS_NAME_SHOW$3);[m
[32m+[m
[32m+[m[32m        var callbackRemove = function callbackRemove() {[m
[32m+[m[32m          _this9._removeBackdrop();[m
[32m+[m
[32m+[m[32m          if (callback) {[m
[32m+[m[32m            callback();[m
[32m+[m[32m          }[m
[32m+[m[32m        };[m
[32m+[m
[32m+[m[32m        if ($__default['default'](this._element).hasClass(CLASS_NAME_FADE$1)) {[m
[32m+[m[32m          var _backdropTransitionDuration = Util.getTransitionDurationFromElement(this._backdrop);[m
[32m+[m
[32m+[m[32m          $__default['default'](this._backdrop).one(Util.TRANSITION_END, callbackRemove).emulateTransitionEnd(_backdropTransitionDuration);[m
[32m+[m[32m        } else {[m
[32m+[m[32m          callbackRemove();[m
[32m+[m[32m        }[m
[32m+[m[32m      } else if (callback) {[m
[32m+[m[32m        callback();[m
[32m+[m[32m      }[m
[32m+[m[32m    } // ----------------------------------------------------------------------[m
[32m+[m[32m    // the following methods are used to handle overflowing modals[m
[32m+[m[32m    // todo (fat): these should probably be refactored out of modal.js[m
[32m+[m[32m    // ----------------------------------------------------------------------[m
[32m+[m[32m    ;[m
[32m+[m
[32m+[m[32m    _proto._adjustDialog = function _adjustDialog() {[m
[32m+[m[32m      var isModalOverflowing = this._element.scrollHeight > document.documentElement.clientHeight;[m
[32m+[m
[32m+[m[32m      if (!this._isBodyOverflowing && isModalOverflowing) {[m
[32m+[m[32m        this._element.style.paddingLeft = this._scrollbarWidth + "px";[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (this._isBodyOverflowing && !isModalOverflowing) {[m
[32m+[m[32m        this._element.style.paddingRight = this._scrollbarWidth + "px";[m
[32m+[m[32m      }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    _proto._resetAdjustments = function _resetAdjustments() {[m
[32m+[m[32m      this._element.style.paddingLeft = '';[m
[32m+[m[32m      this._element.style.paddingRight = '';[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    _proto._checkScrollbar = function _checkScrollbar() {[m
[32m+[m[32m      var rect = document.body.getBoundingClientRect();[m
[32m+[m[32m      this._isBodyOverflowing = Math.round(rect.left + rect.right) < window.innerWidth;[m
[32m+[m[32m      this._scrollbarWidth = this._getScrollbarWidth();[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    _proto._setScrollbar = function _setScrollbar() {[m
[32m+[m[32m      var _this10 = this;[m
[32m+[m
[32m+[m[32m      if (this._isBodyOverflowing) {[m
[32m+[m[32m        // Note: DOMNode.style.paddingRight returns the actual value or '' if not set[m
[32m+[m[32m        //   while $(DOMNode).css('padding-right') returns the calculated value or 0 if not set[m
[32m+[m[32m        var fixedContent = [].slice.call(document.querySelectorAll(SELECTOR_FIXED_CONTENT));[m
[32m+[m[32m        var stickyContent = [].slice.call(document.querySelectorAll(SELECTOR_STICKY_CONTENT)); // Adjust fixed content padding[m
[32m+[m
[32m+[m[32m        $__default['default'](fixedContent).each(function (index, element) {[m
[32m+[m[32m          var actualPadding = element.style.paddingRight;[m
[32m+[m[32m          var calculatedPadding = $__default['default'](element).css('padding-right');[m
[32m+[m[32m          $__default['default'](element).data('padding-right', actualPadding).css('padding-right', parseFloat(calculatedPadding) + _this10._scrollbarWidth + "px");[m
[32m+[m[32m        }); // Adjust sticky content margin[m
[32m+[m
[32m+[m[32m        $__default['default'](stickyContent).each(function (index, element) {[m
[32m+[m[32m          var actualMargin = element.style.marginRight;[m
[32m+[m[32m          var calculatedMargin = $__default['default'](element).css('margin-right');[m
[32m+[m[32m          $__default['default'](element).data('margin-right', actualMargin).css('margin-right', parseFloat(calculatedMargin) - _this10._scrollbarWidth + "px");[m
[32m+[m[32m        }); // Adjust body padding[m
[32m+[m
[32m+[m[32m        var actualPadding = document.body.style.paddingRight;[m
[32m+[m[32m        var calculatedPadding = $__default['default'](document.body).css('padding-right');[m
[32m+[m[32m        $__default['default'](document.body).data('padding-right', actualPadding).css('padding-right', parseFloat(calculatedPadding) + this._scrollbarWidth + "px");[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      $__default['default'](document.body).addClass(CLASS_NAME_OPEN);[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    _proto._resetScrollbar = function _resetScrollbar() {[m
[32m+[m[32m      // Restore fixed content padding[m
[32m+[m[32m      var fixedContent = [].slice.call(document.querySelectorAll(SELECTOR_FIXED_CONTENT));[m
[32m+[m[32m      $__default['default'](fixedContent).each(function (index, element) {[m
[32m+[m[32m        var padding = $__default['default'](element).data('padding-right');[m
[32m+[m[32m        $__default['default'](element).removeData('padding-right');[m
[32m+[m[32m        element.style.paddingRight = padding ? padding : '';[m
[32m+[m[32m      }); // Restore sticky content[m
[32m+[m
[32m+[m[32m      var elements = [].slice.call(document.querySelectorAll("" + SELECTOR_STICKY_CONTENT));[m
[32m+[m[32m      $__default['default'](elements).each(function (index, element) {[m
[32m+[m[32m        var margin = $__default['default'](element).data('margin-right');[m
[32m+[m
[32m+[m[32m        if (typeof margin !== 'undefined') {[m
[32m+[m[32m          $__default['default'](element).css('margin-right', margin).removeData('margin-right');[m
[32m+[m[32m        }[m
[32m+[m[32m      }); // Restore body padding[m
[32m+[m
[32m+[m[32m      var padding = $__default['default'](document.body).data('padding-right');[m
[32m+[m[32m      $__default['default'](document.body).removeData('padding-right');[m
[32m+[m[32m      document.body.style.paddingRight = padding ? padding : '';[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    _proto._getScrollbarWidth = function _getScrollbarWidth() {[m
[32m+[m[32m      // thx d.walsh[m
[32m+[m[32m      var scrollDiv = document.createElement('div');[m
[32m+[m[32m      scrollDiv.className = CLASS_NAME_SCROLLBAR_MEASURER;[m
[32m+[m[32m      document.body.appendChild(scrollDiv);[m
[32m+[m[32m      var scrollbarWidth = scrollDiv.getBoundingClientRect().width - scrollDiv.clientWidth;[m
[32m+[m[32m      document.body.removeChild(scrollDiv);[m
[32m+[m[32m      return scrollbarWidth;[m
[32m+[m[32m    } // Static[m
[32m+[m[32m    ;[m
[32m+[m
[32m+[m[32m    Modal._jQueryInterface = function _jQueryInterface(config, relatedTarget) {[m
[32m+[m[32m      return this.each(function () {[m
[32m+[m[32m        var data = $__default['default'](this).data(DATA_KEY$5);[m
[32m+[m
[32m+[m[32m        var _config = _extends({}, Default$3, $__default['default'](this).data(), typeof config === 'object' && config ? config : {});[m
[32m+[m
[32m+[m[32m        if (!data) {[m
[32m+[m[32m          data = new Modal(this, _config);[m
[32m+[m[32m          $__default['default'](this).data(DATA_KEY$5, data);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (typeof config === 'string') {[m
[32m+[m[32m          if (typeof data[config] === 'undefined') {[m
[32m+[m[32m            throw new TypeError("No method named \"" + config + "\"");[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          data[config](relatedTarget);[m
[32m+[m[32m        } else if (_config.show) {[m
[32m+[m[32m          data.show(relatedTarget);[m
[32m+[m[32m        }[m
[32m+[m[32m      });[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    _createClass(Modal, null, [{[m
[32m+[m[32m      key: "VERSION",[m
[32m+[m[32m      get: function get() {[m
[32m+[m[32m        return VERSION$5;[m
[32m+[m[32m      }[m
[32m+[m[32m    }, {[m
[32m+[m[32m      key: "Default",[m
[32m+[m[32m      get: function get() {[m
[32m+[m[32m        return Default$3;[m
[32m+[m[32m      }[m
[32m+[m[32m    }]);[m
[32m+[m
[32m+[m[32m    return Modal;[m
[32m+[m[32m  }();[m
