[32m+[m
[32m+[m[32m    _proto._getConfig = function _getConfig(config) {[m
[32m+[m[32m      config = _extends({}, Default$6, typeof config === 'object' && config ? config : {});[m
[32m+[m
[32m+[m[32m      if (typeof config.target !== 'string' && Util.isElement(config.target)) {[m
[32m+[m[32m        var id = $__default['default'](config.target).attr('id');[m
[32m+[m
[32m+[m[32m        if (!id) {[m
[32m+[m[32m          id = Util.getUID(NAME$8);[m
