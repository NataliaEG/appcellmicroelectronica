[32m+[m[32m    var _proto = Dropdown.prototype;[m
[32m+[m
[32m+[m[32m    // Public[m
[32m+[m[32m    _proto.toggle = function toggle() {[m
[32m+[m[32m      if (this._element.disabled || $__default['default'](this._element).hasClass(CLASS_NAME_DISABLED)) {[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      var isActive = $__default['default'](this._menu).hasClass(CLASS_NAME_SHOW$2);[m
[32m+[m
[32m+[m[32m      Dropdown._clearMenus();[m
[32m+[m
[32m+[m[32m      if (isActive) {[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      this.show(true);[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    _proto.show = function show(usePopper) {[m
[32m+[m[32m      if (usePopper === void 0) {[m
[32m+[m[32m        usePopper = false;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (this._element.disabled || $__default['default'](this._element).hasClass(CLASS_NAME_DISABLED) || $__default['default'](this._menu).hasClass(CLASS_NAME_SHOW$2)) {[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      var relatedTarget = {[m
[32m+[m[32m        relatedTarget: this._element[m
[32m+[m[32m      };[m
[32m+[m[32m      var showEvent = $__default['default'].Event(EVENT_SHOW$1, relatedTarget);[m
[32m+[m
[32m+[m[32m      var parent = Dropdown._getParentFromElement(this._element);[m
[32m+[m
[32m+[m[32m      $__default['default'](parent).trigger(showEvent);[m
[32m+[m
[32m+[m[32m      if (showEvent.isDefaultPrevented()) {[m
[32m+[m[32m        return;[m
[32m+[m[32m      } // Totally disable Popper for Dropdowns in Navbar[m
[32m+[m
[32m+[m
[32m+[m[32m      if (!this._inNavbar && usePopper) {[m
[32m+[m[32m        /**[m
[32m+[m[32m         * Check for Popper dependency[m
[32m+[m[32m         * Popper - https://popper.js.org[m
[32m+[m[32m         */[m
[32m+[m[32m        if (typeof Popper === 'undefined') {[m
[32m+[m[32m          throw new TypeError('Bootstrap\'s dropdowns require Popper (https://popper.js.org)');[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        var referenceElement = this._element;[m
[32m+[m
[32m+[m[32m        if (this._config.reference === 'parent') {[m
[32m+[m[32m          referenceElement = parent;[m
[32m+[m[32m        } else if (Util.isElement(this._config.reference)) {[m
[32m+[m[32m          referenceElement = this._config.reference; // Check if it's jQuery element[m
[32m+[m
[32m+[m[32m          if (typeof this._config.reference.jquery !== 'undefined') {[m
[32m+[m[32m            referenceElement = this._config.reference[0];[m
[32m+[m[32m          }[m
[32m+[m[32m        } // If boundary is not `scrollParent`, then set position to `static`[m
[32m+[m[32m        // to allow the menu to "escape" the scroll parent's boundaries[m
[32m+[m[32m        // https://github.com/twbs/bootstrap/issues/24251[m
[32m+[m
[32m+[m
[32m+[m[32m        if (this._config.boundary !== 'scrollParent') {[m
[32m+[m[32m          $__default['default'](parent).addClass(CLASS_NAME_POSITION_STATIC);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        this._popper = new Popper(referenceElement, this._menu, this._getPopperConfig());[m
[32m+[m[32m      } // If this is a touch-enabled device we add extra[m
[32m+[m[32m      // empty mouseover listeners to the body's immediate children;[m
[32m+[m[32m      // only needed because of broken event delegation on iOS[m
[32m+[m[32m      // https://www.quirksmode.org/blog/archives/2014/02/mouse_event_bub.html[m
[32m+[m
[32m+[m
[32m+[m[32m      if ('ontouchstart' in document.documentElement && $__default['default'](parent).closest(SELECTOR_NAVBAR_NAV).length === 0) {[m
[32m+[m[32m        $__default['default'](document.body).children().on('mouseover', null, $__default['default'].noop);[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      this._element.focus();[m
[32m+[m
[32m+[m[32m      this._element.setAttribute('aria-expanded', true);[m
[32m+[m
[32m+[m[32m      $__default['default'](this._menu).toggleClass(CLASS_NAME_SHOW$2);[m
[32m+[m[32m      $__default['default'](parent).toggleClass(CLASS_NAME_SHOW$2).trigger($__default['default'].Event(EVENT_SHOWN$1, relatedTarget));[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    _proto.hide = function hide() {[m
[32m+[m[32m      if (this._element.disabled || $__default['default'](this._element).hasClass(CLASS_NAME_DISABLED) || !$__default['default'](this._menu).hasClass(CLASS_NAME_SHOW$2)) {[m
