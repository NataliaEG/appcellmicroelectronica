[32m+[m[32m    Dropdown._clearMenus = function _clearMenus(event) {[m
[32m+[m[32m      if (event && (event.which === RIGHT_MOUSE_BUTTON_WHICH || event.type === 'keyup' && event.which !== TAB_KEYCODE)) {[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      var toggles = [].slice.call(document.querySelectorAll(SELECTOR_DATA_TOGGLE$2));[m
[32m+[m
[32m+[m[32m      for (var i = 0, len = toggles.length; i < len; i++) {[m
[32m+[m[32m        var parent = Dropdown._getParentFromElement(toggles[i]);[m
[32m+[m
[32m+[m[32m        var context = $__default['default'](toggles[i]).data(DATA_KEY$4);[m
[32m+[m[32m        var relatedTarget = {[m
[32m+[m[32m          relatedTarget: toggles[i][m
[32m+[m[32m        };[m
[32m+[m
[32m+[m[32m        if (event && event.type === 'click') {[m
[32m+[m[32m          relatedTarget.clickEvent = event;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (!context) {[m
[32m+[m[32m          continue;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        var dropdownMenu = context._menu;[m
[32m+[m
[32m+[m[32m        if (!$__default['default'](parent).hasClass(CLASS_NAME_SHOW$2)) {[m
[32m+[m[32m          continue;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (event && (event.type === 'click' && /input|textarea/i.test(event.target.tagName) || event.type === 'keyup' && event.which === TAB_KEYCODE) && $__default['default'].contains(parent, event.target)) {[m
[32m+[m[32m          continue;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        var hideEvent = $__default['default'].Event(EVENT_HIDE$1, relatedTarget);[m
[32m+[m[32m        $__default['default'](parent).trigger(hideEvent);[m
[32m+[m
[32m+[m[32m        if (hideEvent.isDefaultPrevented()) {[m
[32m+[m[32m          continue;[m
[32m+[m[32m        } // If this is a touch-enabled device we remove the extra[m
[32m+[m[32m        // empty mouseover listeners we added for iOS support[m
[32m+[m
[32m+[m
[32m+[m[32m        if ('ontouchstart' in document.documentElement) {[m
[32m+[m[32m          $__default['default'](document.body).children().off('mouseover', null, $__default['default'].noop);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        toggles[i].setAttribute('aria-expanded', 'false');[m
[32m+[m
[32m+[m[32m        if (context._popper) {[m
[32m+[m[32m          context._popper.destroy();[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        $__default['default'](dropdownMenu).removeClass(CLASS_NAME_SHOW$2);[m
[32m+[m[32m        $__default['default'](parent).removeClass(CLASS_NAME_SHOW$2).trigger($__default['default'].Event(EVENT_HIDDEN$1, relatedTarget));[m
[32m+[m[32m      }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    Dropdown._getParentFromElement = function _getParentFromElement(element) {[m
[32m+[m[32m      var parent;[m
[32m+[m[32m      var selector = Util.getSelectorFromElement(element);[m
[32m+[m
[32m+[m[32m      if (selector) {[m
[32m+[m[32m        parent = document.querySelector(selector);[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      return parent || element.parentNode;[m
[32m+[m[32m    } // eslint-disable-next-line complexity[m
[32m+[m[32m    ;[m
[32m+[m
[32m+[m[32m    Dropdown._dataApiKeydownHandler = function _dataApiKeydownHandler(event) {[m
[32m+[m[32m      // If not input/textarea:[m
[32m+[m[32m      //  - And not a key in REGEXP_KEYDOWN => not a dropdown command[m
[32m+[m[32m      // If input/textarea:[m
[32m+[m[32m      //  - If space key => not a dropdown command[m
[32m+[m[32m      //  - If key is other than escape[m
[32m+[m[32m      //    - If key is not up or down => not a dropdown command[m
[32m+[m[32m      //    - If trigger inside the menu => not a dropdown command[m
[32m+[m[32m      if (/input|textarea/i.test(event.target.tagName) ? event.which === SPACE_KEYCODE || event.which !== ESCAPE_KEYCODE && (event.which !== ARROW_DOWN_KEYCODE && event.which !== ARROW_UP_KEYCODE || $__default['default'](event.target).closest(SELECTOR_MENU).length) : !REGEXP_KEYDOWN.test(event.which)) {[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (this.disabled || $__default['default'](this).hasClass(CLASS_NAME_DISABLED)) {[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      var parent = Dropdown._getParentFromElement(this);[m
[32m+[m
[32m+[m[32m      var isActive = $__default['default'](parent).hasClass(CLASS_NAME_SHOW$2);[m
[32m+[m
[32m+[m[32m      if (!isActive && event.which === ESCAPE_KEYCODE) {[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      event.preventDefault();[m
[32m+[m[32m      event.stopPropagation();[m
[32m+[m
[32m+[m[32m      if (!isActive || event.which === ESCAPE_KEYCODE || event.which === SPACE_KEYCODE) {[m
[32m+[m[32m        if (event.which === ESCAPE_KEYCODE) {[m
[32m+[m[32m          $__default['default'](parent.querySelector(SELECTOR_DATA_TOGGLE$2)).trigger('focus');[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        $__default['default'](this).trigger('click');[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      var items = [].slice.call(parent.querySelectorAll(SELECTOR_VISIBLE_ITEMS)).filter(function (item) {[m
[32m+[m[32m        return $__default['default'](item).is(':visible');[m
[32m+[m[32m      });[m
[32m+[m
[32m+[m[32m      if (items.length === 0) {[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      var index = items.indexOf(event.target);[m
[32m+[m
[32m+[m[32m      if (event.which === ARROW_UP_KEYCODE && index > 0) {[m
[32m+[m[32m        // Up[m
[32m+[m[32m        index--;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (event.which === ARROW_DOWN_KEYCODE && index < items.length - 1) {[m
[32m+[m[32m        // Down[m
[32m+[m[32m        index++;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (index < 0) {[m
[32m+[m[32m        index = 0;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      items[index].focus();[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    _createClass(Dropdown, null, [{[m
[32m+[m[32m      key: "VERSION",[m
[32m+[m[32m      get: function get() {[m
[32m+[m[32m        return VERSION$4;[m
[32m+[m[32m      }[m
[32m+[m[32m    }, {[m
[32m+[m[32m      key: "Default",[m
[32m+[m[32m      get: function get() {[m
[32m+[m[32m        return Default$2;[m
[32m+[m[32m      }[m
[32m+[m[32m    }, {[m
[32m+[m[32m      key: "DefaultType",[m
[32m+[m[32m      get: function get() {[m
[32m+[m[32m        return DefaultType$2;[m
[32m+[m[32m      }[m
[32m+[m[32m    }]);[m
[32m+[m
[32m+[m[32m    return Dropdown;[m
[32m+[m[32m  }();[m
