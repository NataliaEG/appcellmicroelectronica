[32m+[m[32m  var Util = {[m
[32m+[m[32m    TRANSITION_END: 'bsTransitionEnd',[m
[32m+[m[32m    getUID: function getUID(prefix) {[m
[32m+[m[32m      do {[m
[32m+[m[32m        prefix += ~~(Math.random() * MAX_UID); // "~~" acts like a faster Math.floor() here[m
[32m+[m[32m      } while (document.getElementById(prefix));[m
[32m+[m
[32m+[m[32m      return prefix;[m
[32m+[m[32m    },[m
[32m+[m[32m    getSelectorFromElement: function getSelectorFromElement(element) {[m
[32m+[m[32m      var selector = element.getAttribute('data-target');[m
[32m+[m
[32m+[m[32m      if (!selector || selector === '#') {[m
[32m+[m[32m        var hrefAttr = element.getAttribute('href');[m
[32m+[m[32m        selector = hrefAttr && hrefAttr !== '#' ? hrefAttr.trim() : '';[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      try {[m
[32m+[m[32m        return document.querySelector(selector) ? selector : null;[m
[32m+[m[32m      } catch (_) {[m
[32m+[m[32m        return null;[m
[32m+[m[32m      }[m
[32m+[m[32m    },[m
[32m+[m[32m    getTransitionDurationFromElement: function getTransitionDurationFromElement(element) {[m
[32m+[m[32m      if (!element) {[m
[32m+[m[32m        return 0;[m
[32m+[m[32m      } // Get transition-duration of the element[m
[32m+[m
[32m+[m
[32m+[m[32m      var transitionDuration = $__default['default'](element).css('transition-duration');[m
[32m+[m[32m      var transitionDelay = $__default['default'](element).css('transition-delay');[m
[32m+[m[32m      var floatTransitionDuration = parseFloat(transitionDuration);[m
[32m+[m[32m      var floatTransitionDelay = parseFloat(transitionDelay); // Return 0 if element or transition duration is not found[m
[32m+[m
[32m+[m[32m      if (!floatTransitionDuration && !floatTransitionDelay) {[m
[32m+[m[32m        return 0;[m
[32m+[m[32m      } // If multiple durations are defined, take the first[m
[32m+[m
[32m+[m
[32m+[m[32m      transitionDuration = transitionDuration.split(',')[0];[m
[32m+[m[32m      transitionDelay = transitionDelay.split(',')[0];[m
[32m+[m[32m      return (parseFloat(transitionDuration) + parseFloat(transitionDelay)) * MILLISECONDS_MULTIPLIER;[m
[32m+[m[32m    },[m
[32m+[m[32m    reflow: function reflow(element) {[m
[32m+[m[32m      return element.offsetHeight;[m
[32m+[m[32m    },[m
[32m+[m[32m    triggerTransitionEnd: function triggerTransitionEnd(element) {[m
[32m+[m[32m      $__default['default'](element).trigger(TRANSITION_END);[m
[32m+[m[32m    },[m
[32m+[m[32m    supportsTransitionEnd: function supportsTransitionEnd() {[m
[32m+[m[32m      return Boolean(TRANSITION_END);[m
[32m+[m[32m    },[m
[32m+[m[32m    isElement: function isElement(obj) {[m
[32m+[m[32m      return (obj[0] || obj).nodeType;[m
[32m+[m[32m    },[m
[32m+[m[32m    typeCheckConfig: function typeCheckConfig(componentName, config, configTypes) {[m
[32m+[m[32m      for (var property in configTypes) {[m
[32m+[m[32m        if (Object.prototype.hasOwnProperty.call(configTypes, property)) {[m
[32m+[m[32m          var expectedTypes = configTypes[property];[m
[32m+[m[32m          var value = config[property];[m
[32m+[m[32m          var valueType = value && Util.isElement(value) ? 'element' : toType(value);[m
[32m+[m
[32m+[m[32m          if (!new RegExp(expectedTypes).test(valueType)) {[m
[32m+[m[32m            throw new Error(componentName.toUpperCase() + ": " + ("Option \"" + property + "\" provided type \"" + valueType + "\" ") + ("but expected type \"" + expectedTypes + "\"."));[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    },[m
[32m+[m[32m    findShadowRoot: function findShadowRoot(element) {[m
[32m+[m[32m      if (!document.documentElement.attachShadow) {[m
[32m+[m[32m        return null;[m
[32m+[m[32m      } // Can find the shadow root otherwise it'll return the document[m
[32m+[m
[32m+[m
[32m+[m[32m      if (typeof element.getRootNode === 'function') {[m
[32m+[m[32m        var root = element.getRootNode();[m
[32m+[m[32m        return root instanceof ShadowRoot ? root : null;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (element instanceof ShadowRoot) {[m
[32m+[m[32m        return element;[m
[32m+[m[32m      } // when we don't find a shadow root[m
[32m+[m
[32m+[m
[32m+[m[32m      if (!element.parentNode) {[m
[32m+[m[32m        return null;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      return Util.findShadowRoot(element.parentNode);[m
[32m+[m[32m    },[m
[32m+[m[32m    jQueryDetection: function jQueryDetection() {[m
[32m+[m[32m      if (typeof $__default['default'] === 'undefined') {[m
[32m+[m[32m        throw new TypeError('Bootstrap\'s JavaScript requires jQuery. jQuery must be included before Bootstrap\'s JavaScript.');[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      var version = $__default['default'].fn.jquery.split(' ')[0].split('.');[m
[32m+[m[32m      var minMajor = 1;[m
[32m+[m[32m      var ltMajor = 2;[m
[32m+[m[32m      var minMinor = 9;[m
[32m+[m[32m      var minPatch = 1;[m
[32m+[m[32m      var maxMajor = 4;[m
[32m+[m
[32m+[m[32m      if (version[0] < ltMajor && version[1] < minMinor || version[0] === minMajor && version[1] === minMinor && version[2] < minPatch || version[0] >= maxMajor) {[m
[32m+[m[32m        throw new Error('Bootstrap\'s JavaScript requires at least jQuery v1.9.1 but less than v4.0.0');[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m[32m  Util.jQueryDetection();[m
[32m+[m[32m  setTransitionEndSupport();[m
