[32m+[m[32m<?php[m
[32m+[m[32minclude("../conexion.php");[m
[32m+[m
[32m+[m[32m// Obtener el ID del cliente del cuerpo de la solicitud[m
[32m+[m[32m$data = json_decode(file_get_contents('php://input'), true);[m
[32m+[m[32m$idCliente = $data['numero'] ?? null;[m
[32m+[m
[32m+[m[32m$response = [[m
[32m+[m[32m  'success' => false,[m
[32m+[m[32m  'error' => 'No se proporcionó el ID del cliente.'[m
[32m+[m[32m];[m
[32m+[m
[32m+[m[32mif ($idCliente) {[m
[32m+[m[32m  $sql = "DELETE FROM trabajos WHERE numero = $idCliente";[m
[32m+[m
[32m+[m[32m  if (mysqli_query($con, $sql)) {[m
[32m+[m[32m    $response = [[m
[32m+[m[32m      'success' => true[m
[32m+[m[32m    ];[m
[32m+[m[32m  } else {[m
[32m+[m[32m    $response['error'] = 'Error al eliminar el cliente: ' . mysqli_error($con);[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mheader('Content-Type: application/json');[m
[32m+[m[32mecho json_encode($response);[m
