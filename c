[36m@@ -0,0 +1,40 @@[m
[32m+[m[32m<?php[m
[32m+[m[32minclude("../conexion.php");[m
[32m+[m
[32m+[m[32m$cliente = $_POST['modal-cliente'] ?? null;[m[41m [m
[32m+[m[32m$telefono = $_POST['modal-telefono'] ?? null;[m
[32m+[m[32m$codigo = $_POST['modal-codigo'] ?? null;[m
[32m+[m[32m$modelo = $_POST['modal-modelo'] ?? null;[m
[32m+[m[32m$falla = $_POST['modal-falla'] ?? null;[m
[32m+[m[32m$observacion = $_POST['modal-observacion'] ?? null;[m[41m [m
[32m+[m[32m$fecha_ingreso = $_POST['modal-fecha_ingreso'] ?? null;[m
[32m+[m[32m$fecha_entrega = $_POST['modal-fecha_entrega'] ?? null;[m
[32m+[m[32m$precio = $_POST['modal-precio'] ?? null;[m
[32m+[m[32m$imei = $_POST['modal-imei'] ?? null;[m
[32m+[m[32m$estado = $_POST['modal-estado'] ?? null;[m[41m [m
[32m+[m
[32m+[m[32m$response = [[m
[32m+[m[32m    'success' => false,[m
[32m+[m[32m    'error' => 'No se proporcionaron todos los datos requeridos.'[m
[32m+[m[32m];[m
[32m+[m
[32m+[m[32mif ([m
[32m+[m[32m    $cliente && $codigo && $modelo && $falla && $observacion && $fecha_ingreso && $fecha_entrega && $precio && $imei && $estado[m
[32m+[m[32m) {[m
[32m+[m[32m    $sql = "INSERT INTO trabajos (cliente, codigo, telefono, modelo, falla, observacion, fecha_ingreso, fecha_entrega, precio, imei, estado)[m[41m [m
[32m+[m[32m                        VALUES ('$cliente', '$codigo', '$telefono', '$modelo', '$falla', '$observacion', '$fecha_ingreso', '$fecha_entrega', '$precio', '$imei', '$estado')";[m
[32m+[m
[32m+[m[32m    if (mysqli_query($con, $sql)) {[m
[32m+[m[32m        $response = [[m
[32m+[m[32m            'success' => true[m
[32m+[m[32m        ];[m
[32m+[m[32m    } else {[m
[32m+[m[32m        $response['error'] = 'Error al insertar el cliente en la base de datos: ' . mysqli_error($con);[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mheader('Content-Type: application/json');[m
[32m+[m[32mecho json_encode($response);[m
[32m+[m
[32m+[m[32mmysqli_close($con);[m
[32m+[m[32m?>[m
