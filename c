[32m+[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (initialButton.tagName === 'INPUT' || button.tagName !== 'LABEL') {[m
[32m+[m[32m        Button._jQueryInterface.call($__default['default'](button), 'toggle', initialButton.tagName === 'INPUT');[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
