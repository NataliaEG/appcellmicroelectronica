[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    _proto.toggle = function toggle(event) {[m
[32m+[m[32m      if (!this._isEnabled) {[m
[32m+[m[32m        return;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (event) {[m
[32m+[m[32m        var dataKey = this.constructor.DATA_KEY;[m
[32m+[m[32m        var context = $__default['default'](event.currentTarget).data(dataKey);[m
[32m+[m
[32m+[m[32m        if (!context) {[m
[32m+[m[32m          context = new this.constructor(event.currentTarget, this._getDelegateConfig());[m
[32m+[m[32m          $__default['default'](event.currentTarget).data(dataKey, context);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        context._activeTrigger.click = !context._activeTrigger.click;[m
[32m+[m
[32m+[m[32m        if (context._isWithActiveTrigger()) {[m
[32m+[m[32m          context._enter(null, context);[m
[32m+[m[32m        } else {[m
[32m+[m[32m          context._leave(null, context);[m
[32m+[m[32m        }[m
[32m+[m[32m      } else {[m
[32m+[m[32m        if ($__default['default'](this.getTipElement()).hasClass(CLASS_NAME_SHOW$4)) {[m
[32m+[m[32m          this._leave(null, this);[m
[32m+[m
[32m+[m[32m          return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        this._enter(null, this);[m
[32m+[m[32m      }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    _proto.dispose = function dispose() {[m
[32m+[m[32m      clearTimeout(this._timeout);[m
