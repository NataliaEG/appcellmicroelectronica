[32m+[m[32m<?php[m
[32m+[m[32minclude("../conexion.php");[m
[32m+[m
[32m+[m[32m$consulta = "SELECT numero, cliente, telefono, codigo, modelo, falla, observacion, fecha_ingreso, fecha_entrega, precio, imei, estado FROM trabajos";[m
[32m+[m[32m$resultado = mysqli_query($con, $consulta);[m
[32m+[m
[32m+[m[32mif ($resultado && mysqli_num_rows($resultado) > 0) {[m
[32m+[m[32m    $response = [];[m
[32m+[m[32m    while ($fila = mysqli_fetch_assoc($resultado)) {[m
[32m+[m[32m        $numero = $fila['numero'];[m
[32m+[m[32m        $cliente = $fila['cliente'];[m
[32m+[m[32m        $telefono = $fila['telefono'];[m
[32m+[m[32m        $codigo = $fila['codigo'];[m
[32m+[m[32m        $modelo = $fila['modelo'];[m
[32m+[m[32m        $falla = $fila['falla'];[m
[32m+[m[32m        $observacion = $fila['observacion'];[m
[32m+[m[32m        $fecha_ingreso = $fila['fecha_ingreso'];[m
[32m+[m[32m        $fecha_entrega = $fila['fecha_entrega'];[m
[32m+[m[32m        $precio = $fila['precio'];[m
[32m+[m[32m        $imei = $fila['imei'];[m
[32m+[m[32m        $estado = $fila['estado'];[m
[32m+[m
[32m+[m[32m        $response[] = [[m
[32m+[m[32m            'numero' => $numero,[m
[32m+[m[32m            'cliente' => $cliente,[m
[32m+[m[32m            'telefono' => $telefono,[m
[32m+[m[32m            'codigo' => $codigo,[m
[32m+[m[32m            'modelo' => $modelo,[m
[32m+[m[32m            'falla' => $falla,[m
[32m+[m[32m            'observacion' => $observacion,[m
[32m+[m[32m            'fecha_ingreso' => $fecha_ingreso,[m
[32m+[m[32m            'fecha_entrega' => $fecha_entrega,[m
[32m+[m[32m            'precio' => $precio,[m
[32m+[m[32m            'imei' => $imei,[m
[32m+[m[32m            'estado' => $estado[m
[32m+[m[32m        ];[m
[32m+[m[32m    }[m
[32m+[m[32m} else {[m
[32m+[m[32m    $response = [[m
[32m+[m[32m        'mensaje' => 'Error: No se pueden cargar los datos'[m
[32m+[m[32m    ];[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mmysqli_close($con);[m
[32m+[m
[32m+[m[32mheader('Content-Type: application/json');[m
[32m+[m[32mecho json_encode($response);[m
[32m+[m[32m?>[m
