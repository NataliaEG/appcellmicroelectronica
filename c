[32m+[m[32m  /**[m
[32m+[m[32m   * A pattern that recognizes a commonly useful subset of URLs that are safe.[m
[32m+[m[32m   *[m
[32m+[m[32m   * Shoutout to Angular 7 https://github.com/angular/angular/blob/7.2.4/packages/core/src/sanitization/url_sanitizer.ts[m
[32m+[m[32m   */[m
[32m+[m
[32m+[m[32m  var SAFE_URL_PATTERN = /^(?:(?:https?|mailto|ftp|tel|file):|[^#&/:?]*(?:[#/?]|$))/gi;[m
[32m+[m[32m  /**[m
[32m+[m[32m   * A pattern that matches safe data URLs. Only matches image, video and audio types.[m
[32m+[m[32m   *[m
[32m+[m[32m   * Shoutout to Angular 7 https://github.com/angular/angular/blob/7.2.4/packages/core/src/sanitization/url_sanitizer.ts[m
[32m+[m[32m   */[m
[32m+[m
[32m+[m[32m  var DATA_URL_PATTERN = /^data:(?:image\/(?:bmp|gif|jpeg|jpg|png|tiff|webp)|video\/(?:mpeg|mp4|ogg|webm)|audio\/(?:mp3|oga|ogg|opus));base64,[\d+/a-z]+=*$/i;[m
[32m+[m
[32m+[m[32m  function allowedAttribute(attr, allowedAttributeList) {[m
[32m+[m[32m    var attrName = attr.nodeName.toLowerCase();[m
[32m+[m
[32m+[m[32m    if (allowedAttributeList.indexOf(attrName) !== -1) {[m
[32m+[m[32m      if (uriAttrs.indexOf(attrName) !== -1) {[m
[32m+[m[32m        return Boolean(attr.nodeValue.match(SAFE_URL_PATTERN) || attr.nodeValue.match(DATA_URL_PATTERN));[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      return true;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var regExp = allowedAttributeList.filter(function (attrRegex) {[m
[32m+[m[32m      return attrRegex instanceof RegExp;[m
[32m+[m[32m    }); // Check if a regular expression validates the attribute.[m
[32m+[m
[32m+[m[32m    for (var i = 0, len = regExp.length; i < len; i++) {[m
[32m+[m[32m      if (attrName.match(regExp[i])) {[m
[32m+[m[32m        return true;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return false;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  function sanitizeHtml(unsafeHtml, whiteList, sanitizeFn) {[m
[32m+[m[32m    if (unsafeHtml.length === 0) {[m
[32m+[m[32m      return unsafeHtml;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (sanitizeFn && typeof sanitizeFn === 'function') {[m
[32m+[m[32m      return sanitizeFn(unsafeHtml);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var domParser = new window.DOMParser();[m
[32m+[m[32m    var createdDocument = domParser.parseFromString(unsafeHtml, 'text/html');[m
[32m+[m[32m    var whitelistKeys = Object.keys(whiteList);[m
[32m+[m[32m    var elements = [].slice.call(createdDocument.body.querySelectorAll('*'));[m
[32m+[m
[32m+[m[32m    var _loop = function _loop(i, len) {[m
[32m+[m[32m      var el = elements[i];[m
[32m+[m[32m      var elName = el.nodeName.toLowerCase();[m
[32m+[m
[32m+[m[32m      if (whitelistKeys.indexOf(el.nodeName.toLowerCase()) === -1) {[m
[32m+[m[32m        el.parentNode.removeChild(el);[m
[32m+[m[32m        return "continue";[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      var attributeList = [].slice.call(el.attributes);[m
[32m+[m[32m      var whitelistedAttributes = [].concat(whiteList['*'] || [], whiteList[elName] || []);[m
[32m+[m[32m      attributeList.forEach(function (attr) {[m
[32m+[m[32m        if (!allowedAttribute(attr, whitelistedAttributes)) {[m
[32m+[m[32m          el.removeAttribute(attr.nodeName);[m
[32m+[m[32m        }[m
[32m+[m[32m      });[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    for (var i = 0, len = elements.length; i < len; i++) {[m
[32m+[m[32m      var _ret = _loop(i);[m
[32m+[m
[32m+[m[32m      if (_ret === "continue") continue;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return createdDocument.body.innerHTML;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  /**[m
[32m+[m[32m   * ------------------------------------------------------------------------[m
[32m+[m[32m   * Constants[m
[32m+[m[32m   * ------------------------------------------------------------------------[m
[32m+[m[32m   */[m
[32m+[m
[32m+[m[32m  var NAME$6 = 'tooltip';[m
