[32m+[m
[32m+[m
[32m+[m[32m  $__default['default'](document).on(EVENT_CLICK_DATA_API$1, SELECTOR_DATA_TOGGLE_CARROT, function (event) {[m
[32m+[m[32m    var button = event.target;[m
[32m+[m[32m    var initialButton = button;[m
[32m+[m
[32m+[m[32m    if (!$__default['default'](button).hasClass(CLASS_NAME_BUTTON)) {[m
[32m+[m[32m      button = $__default['default'](button).closest(SELECTOR_BUTTON)[0];[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (!button || button.hasAttribute('disabled') || button.classList.contains('disabled')) {[m
[32m+[m[32m      event.preventDefault(); // work around Firefox bug #1540995[m
[32m+[m[32m    } else {[m
