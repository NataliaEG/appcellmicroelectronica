[32m+[m[32m  var Button = /*#__PURE__*/function () {[m
[32m+[m[32m    function Button(element) {[m
[32m+[m[32m      this._element = element;[m
[32m+[m[32m      this.shouldAvoidTriggerChange = false;[m
[32m+[m[32m    } // Getters[m
[32m+[m
[32m+[m
[32m+[m[32m    var _proto = Button.prototype;[m
[32m+[m
[32m+[m[32m    // Public[m
[32m+[m[32m    _proto.toggle = function toggle() {[m
[32m+[m[32m      var triggerChangeEvent = true;[m
[32m+[m[32m      var addAriaPressed = true;[m
[32m+[m[32m      var rootElement = $__default['default'](this._element).closest(SELECTOR_DATA_TOGGLES)[0];[m
[32m+[m
[32m+[m[32m      if (rootElement) {[m
[32m+[m[32m        var input = this._element.querySelector(SELECTOR_INPUT);[m
[32m+[m
[32m+[m[32m        if (input) {[m
[32m+[m[32m          if (input.type === 'radio') {[m
[32m+[m[32m            if (input.checked && this._element.classList.contains(CLASS_NAME_ACTIVE)) {[m
[32m+[m[32m              triggerChangeEvent = false;[m
[32m+[m[32m            } else {[m
[32m+[m[32m              var activeElement = rootElement.querySelector(SELECTOR_ACTIVE);[m
[32m+[m
[32m+[m[32m              if (activeElement) {[m
[32m+[m[32m                $__default['default'](activeElement).removeClass(CLASS_NAME_ACTIVE);[m
[32m+[m[32m              }[m
[32m+[m[32m            }[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          if (triggerChangeEvent) {[m
[32m+[m[32m            // if it's not a radio button or checkbox don't add a pointless/invalid checked property to the input[m
[32m+[m[32m            if (input.type === 'checkbox' || input.type === 'radio') {[m
[32m+[m[32m              input.checked = !this._element.classList.contains(CLASS_NAME_ACTIVE);[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            if (!this.shouldAvoidTriggerChange) {[m
[32m+[m[32m              $__default['default'](input).trigger('change');[m
[32m+[m[32m            }[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          input.focus();[m
[32m+[m[32m          addAriaPressed = false;[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      if (!(this._element.hasAttribute('disabled') || this._element.classList.contains('disabled'))) {[m
[32m+[m[32m        if (addAriaPressed) {[m
[32m+[m[32m          this._element.setAttribute('aria-pressed', !this._element.classList.contains(CLASS_NAME_ACTIVE));[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (triggerChangeEvent) {[m
[32m+[m[32m          $__default['default'](this._element).toggleClass(CLASS_NAME_ACTIVE);[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    _proto.dispose = function dispose() {[m
[32m+[m[32m      $__default['default'].removeData(this._element, DATA_KEY$1);[m
[32m+[m[32m      this._element = null;[m
[32m+[m[32m    } // Static[m
[32m+[m[32m    ;[m
[32m+[m
[32m+[m[32m    Button._jQueryInterface = function _jQueryInterface(config, avoidTriggerChange) {[m
[32m+[m[32m      return this.each(function () {[m
[32m+[m[32m        var $element = $__default['default'](this);[m
[32m+[m[32m        var data = $element.data(DATA_KEY$1);[m
[32m+[m
[32m+[m[32m        if (!data) {[m
[32m+[m[32m          data = new Button(this);[m
[32m+[m[32m          $element.data(DATA_KEY$1, data);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        data.shouldAvoidTriggerChange = avoidTriggerChange;[m
[32m+[m
[32m+[m[32m        if (config === 'toggle') {[m
[32m+[m[32m          data[config]();[m
[32m+[m[32m        }[m
[32m+[m[32m      });[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    _createClass(Button, null, [{[m
[32m+[m[32m      key: "VERSION",[m
[32m+[m[32m      get: function get() {[m
[32m+[m[32m        return VERSION$1;[m
[32m+[m[32m      }[m
[32m+[m[32m    }]);[m
[32m+[m
[32m+[m[32m    return Button;[m
[32m+[m[32m  }();[m
[32m+[m[32m  /**[m
